<?xml version="1.0"?>
<launch>
  <arg name="yaml_config" default="$(find ur_description)/config/ur5_default.yaml"/>
  <arg name="transmission_hw_interface" default="hardware_interface/PositionJointInterface" />

  <param name="robot_description" command="$(find xacro)/xacro --inorder '$(find ur_description)/urdf/ur5_robot_yaml.urdf.xacro' yaml_config:=$(arg yaml_config)" transmission_hw_interface:=$(arg transmission_hw_interface)" />
</launch>
